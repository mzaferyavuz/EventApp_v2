<div class="row " id="dataSelection">
    <div class="col-md-12">
        <div class="portlet box red">
            <div class="portlet-title">
                <div class="caption">
                    <i class="fa fa-wrench"></i>Secim
                </div>
            </div>
            <div class="portlet-body">
                <input type="hidden" id="pageRequesterId" value="1" />
                <div class="form-group">

                    <div class="row">

                        @*<label class="control-label col-md-2"></label>*@
                        @**@
                        <div class="form-group">
                            <div class="input-group select2-bootstrap-prepend">
                                <span class="input-group-addon">
                                    <input type="checkbox" name="allArea" id="allArea">
                                </span>

                                <select id="span_small" class="form-control select2 zoneSelection select2-selection__clear">

                                    <optgroup label="Bolge">
                                        @*<option value="All">All</option>*@

                                        @for (int i = 0; i < Model.AreaNames.Count; i++)
                                        {
                                            <option value="@Model.AreaNames[i]">@Model.AreaNames[i]</option>
                                        }
                                    </optgroup>
                                </select>

                            </div>
                        </div>
                        <div class="form-group">
                            <div class="input-group select2-bootstrap-prepend">
                                <span class="input-group-addon">
                                    <input type="checkbox" name="allSource" id="allSource" />
                                </span>



                                <select id="span_small" class="form-control select2 sourceSelection  select2-selection__clear">
                                    <optgroup label="Kaynak">

                                        @for (int i = 0; i < Model.Sources.Count; i++)
                                        {
                                            <option value="@Model.Sources[i]">@Model.Sources[i]</option>
                                        }
                                    </optgroup>
                                </select>
                            </div>
                        </div>
                    </div><br />
                    <div class="row">
                        <div class="form-group">

                            <label class="control-label col-md-4"></label>
                            <div class="col-md-4">
                                <div class="input-group input-large date-picker input-daterange" data-date="10/11/2012" data-date-format="mm/dd/yyyy">
                                    <input type="text" class="form-control startingDate" name="from">
                                    <span class="input-group-addon"> to </span>
                                    <input type="text" class="form-control endingDate" name="to">
                                </div>
                                <!-- /input-group -->
                                @*<span class="help-block"> Tarih Aralığı Seçinizi </span>*@
                            </div>

                        </div><br />
                    </div><br />
                    <form class="form-horizontal">
                        <div class="col-sm-offset-2 col-sm-10">
                            <label class="mt-checkbox mt-checkbox-outline">
                                <input type="checkbox" id="max1"> MAX 1
                                <span></span>
                            </label>
                        </div>
                        <div class="col-sm-offset-2 col-sm-10">
                            <label class="mt-checkbox mt-checkbox-outline">
                                <input type="checkbox" id="max2"> MAX 2
                                <span></span>
                            </label>
                        </div>
                        <div class="col-sm-offset-2 col-sm-10">
                            <label class="mt-checkbox mt-checkbox-outline">
                                <input type="checkbox" id="min1"> MIN 1
                                <span></span>
                            </label>
                        </div>
                        <div class="col-sm-offset-2 col-sm-10">
                            <label class="mt-checkbox mt-checkbox-outline">
                                <input type="checkbox" id="min2"> MIN 2
                                <span></span>
                            </label>
                        </div>
                        <div class="col-sm-offset-2 col-sm-10">
                            <label class="mt-checkbox mt-checkbox-outline">
                                <input type="checkbox" id="hiRange"> HiRange
                                <span></span>
                            </label>
                        </div>
                        <div class="col-sm-offset-2 col-sm-10">
                            <label class="mt-checkbox mt-checkbox-outline">
                                <input type="checkbox" id="loRange"> LoRange
                                <span></span>
                            </label>
                        </div>
                        <div class="col-sm-offset-2 col-sm-10">
                            <label class="mt-checkbox mt-checkbox-outline">
                                <input type="checkbox" id="alrmFault"> Alarm/Fault
                                <span></span>
                            </label>
                        </div>
                    </form>
                    <div class="form-group">
                        <input id="searchQuery" type="text" class="form-control" placeholder="Kelimeler arasında Ve sorgusu için . Veya sorgusu için , işaretlerini kullanınız. ">
                    </div>
                    <div class="row">
                        <label class="control-label col-md-5"></label>
                        <div class="col-md-4">
                            <a class="btn btn-circle btn-sm green getDetails">
                                Detaylar
                                <i class="fa fa-user "> </i>
                            </a>
                        </div>
                    </div>

                </div>



            </div>
        </div>
    </div>
</div>


<div class="row" id="activityLog">
    <div class="col-md-12">
        <!-- BEGIN Portlet PORTLET-->
        <div class="portlet box green">
            <div class="portlet-title">
                <div class="caption">
                    <i class="fa fa-globe"></i>Event Details
                </div>

            </div>
            <div class="portlet-body">
                <table class="table table-striped table-bordered table-hover" id="eventDetails">
                    <thead>
                        <tr>
                            <th> Bolge </th>
                            <th> Nokta </th>
                            <th> Durum </th>
                            <th> Aciklama </th>
                            <th> Baslangic</th>
                            <th> Bitis Tarihi</th>
                            <th> Gecen Zaman</th>
                            <th> Reason</th>
                        </tr>
                    </thead>
                    <tbody>
                    </tbody>
                </table>
            </div>
        </div>
        <!-- END Portlet PORTLET-->
    </div>
</div>




